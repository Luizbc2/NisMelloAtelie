{
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php@0.7.2"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|svg|ico|json))$",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.php?page=home"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php?page=$1"
    }
  ]
}
