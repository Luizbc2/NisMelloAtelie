{
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php@0.7.2"
    }
  ],
  "routes": [
    {
      "src": "/(styles\\.css|script\\.js|imagens/.*|images/.*)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.php?page=home"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php?page=$1"
    }
  ]
}
